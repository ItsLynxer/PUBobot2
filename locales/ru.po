# bot/queue_channel.py

msgid "no players"
msgstr "нет игроков"

msgid "You must possess admin permissions."
msgstr "Необходимы права администратора."

msgid "You must possess moderator permissions."
msgstr "Необходимы права модератора."

msgid "expire time ran off"
msgstr "время expire истекло"

